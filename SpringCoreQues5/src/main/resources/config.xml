<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="complexBean" class="com.spring.assignment.Complex">
        <property name="list">
            <list>
                <value>1</value>
                <value>2</value>
            </list>
        </property>

        <property name="set">
            <set>
                <value>1</value>
                <value>2</value>
                <value>1</value>
                <value>4</value>
            </set>
        </property>

        <property name="map">
            <map>
                <entry key="1" value="sargam"/>
                <entry key="2" value="sachdeva"/>
            </map>
        </property>

    </bean>
</beans>